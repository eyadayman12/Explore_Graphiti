<node id="n19" labels=":Entity"><data key="labels">:Entity</data><data key="uuid">cdce50d9-4f7b-4cde-9579-6ac90ade3a2b</data><data key="name">العلوم</data><data key="created_at">2025-07-23T14:10:15.244085Z[UTC]</data><data key="group_id"></data><data key="summary">الرسائل تتناول مواضيع متعددة تتعلق باللغة العربية، تدريسها، تاريخها، وأهميتها في الحضارة الإسلامية، بالإضافة إلى مواضيع علمية وتقنية، مع التركيز على أهمية فهم المعاني، الدراسات العميقة، وتاريخ الترجمة، والعلوم الإسلامية، وتطوير المهارات العلمية والتقنية، مع إبراز دور اللغة العربية في العالم الإسلامي وتاريخها وطرق تدريسها.</data><data key="name_embedding">[0.022341674,-0.038329106,0.05006001,0.051485643,0.019164553,0.036516517,-0.014185028,0.011914201,-0.027575776,-0.044316757,0.0044016354,-0.027636874,-0.057351097,0.014877478,0.052218825,0.03313574,-0.05898039,0.022973025,0.03692384,0.012474271,0.04132293,-0.009684107,-0.011486513,-0.030589966,-0.011629076,-0.035620406,0.036862742,0.021160437,-0.0042794384,-0.006537536,0.0652939,-0.034785394,0.010376557,-0.009729931,0.022300942,0.044276025,0.023828402,-0.05006001,0.0031567542,-0.016231827,-0.023013758,-0.023991331,-0.051852234,0.013686057,0.03507052,0.02983642,-0.010794063,0.01412393,0.022810096,-0.022321308,0.011455963,-0.024765246,0.031038022,0.025681723,0.012270609,0.054907158,-0.007204528,0.002340835,0.03794215,-0.026516736,-0.03474466,-0.0069550425,0.02861445,0.012402989,-0.013085255,-0.013146354,0.021343732,-0.009012024,-0.0021231717,-0.058165744,0.024561584,-0.011985483,-0.02405243,0.022973025,-0.04085451,-0.0107227815,-0.05352226,-7.484562E-4,0.0014803649,0.010045607,-0.02305449,-0.013706423,0.0092716925,0.036007363,0.012260426,-0.033930015,-0.08142389,0.0024324828,-0.039673273,0.01610963,-0.019510778,-0.008670891,-0.032565486,0.0041266927,-0.0063796984,1.8345453E-4,-0.005559961,-0.015671758,0.018614667,0.03217853,-0.034296606,-0.030080814,0.0021460836,0.034683563,0.0015745584,0.007886794,-5.874363E-4,-0.07429574,-0.03894009,0.040773045,0.002302648